<div class="eui-window">
  <div class="eui-windowwrapper">
    <ul class="eui-options">
      {{#each actionList}}
        {{#unless primary}}
        <li class="option" {{action 'actionThenHide' this.action}}>{{label}}</li>
        {{/unless}}
      {{/each}}
    </ul>
  </div>
</div>

<div class="eui-overlay" {{action 'closePopup'}}></div>
