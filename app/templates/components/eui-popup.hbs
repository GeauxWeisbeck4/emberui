<div class="eui-popupwrapper">
  {{#if searchString}}
    <div class="eui-search-wrapper">
      {{input class="eui-search" valueBinding="searchString" size="1"}}
    </div>
  {{/if}}

  {{#if hasNoOptions}}
    <div class="eui-nooptions">No results found.</div>
  {{else}}
    {{view listView contentBinding="filteredOptions"}}
  {{/if}}
</div>
